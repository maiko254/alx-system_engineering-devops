server {
     listen      80 default_server;
     listen      [::]:80 default_server;
     listen      8080;
     root        /etc/nginx/html;
     add_header X-Served-By 108938-web-01;
     location / {
        index mb-bnb.html index.html index.htm;
     }
     location /redirect_me {
        return 301 http://msn.com;
     }
     error_page 404 /404.html;
     location /404 {
        root /etc/nginx/html;
        internal;
     }
}
